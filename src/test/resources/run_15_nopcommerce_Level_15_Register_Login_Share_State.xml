<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="none" name="Nopcommerce - Administrator">
	<listeners>
		<listener class-name="commons.MethodListener" />
		<listener class-name="reportConfig.AllureTestListener"/>
		  <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
		
	</listeners>
	<parameter name="url" value="https://demo.nopcommerce.com" />

	<test name="Run on chrome" preserve-order="true">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.nopcommerce.common.Level_15_Common01_Register_Login" />
			<class name="com.nopcommerce.user.Level_15_Register_Login_Share_State_Part1_Search" />
			<class name="com.nopcommerce.user.Level_15_Register_Login_Share_State_Part2_Sort" />
		</classes>
	</test>

</suite> 